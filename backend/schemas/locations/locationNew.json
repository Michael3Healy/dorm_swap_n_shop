{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "street",
        "city",
        "state"
    ],
    "properties": {
        "street": {
            "type": "string",
            "default": "",
            "title": "The street Schema",
            "examples": [
                "123 Main St"
            ]
        },
        "city": {
            "type": "string",
            "default": "",
            "title": "The city Schema",
            "examples": [
                "Springfield"
            ]
        },
        "state": {
            "type": "string",
            "default": "",
            "title": "The state Schema",
            "examples": [
                "IL"
            ]
        }
    },
    "additionalProperties": false,
    "examples": [{
        "street": "123 Main St",
        "city": "Springfield",
        "state": "IL"
    }]
}