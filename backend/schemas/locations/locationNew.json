{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/example.json",
	"type": "object",
	"default": {},
	"title": "Root Schema",
	"required": ["latitude", "longitude"],
	"properties": {
		"street": {
			"type": "string",
			"default": "",
			"title": "The street Schema",
			"examples": ["123 Example St"]
		},
		"city": {
			"type": "string",
			"default": "",
			"title": "The city Schema",
			"examples": ["Example City"]
		},
		"state": {
			"type": "string",
			"default": "",
			"title": "The state Schema",
			"examples": ["EX"]
		},
		"zip": {
			"type": "string",
			"default": "",
			"title": "The zip Schema",
			"examples": ["12345"]
		},
		"latitude": {
			"type": "number",
			"minimum": -90,
			"maximum": 90,
			"multipleOf": 0.000001,
			"description": "Latitude with precision up to 6 decimal places."
		},
		"longitude": {
			"type": "number",
			"minimum": -180,
			"maximum": 180,
			"multipleOf": 0.000001,
			"description": "Longitude with precision up to 6 decimal places."
		}
	}
}
