{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [],
    "properties": {
        "image": {
            "type": "string",
            "default": "",
            "title": "The image Schema",
            "examples": [
                "https://example.com/path/to/image.jpg"
            ]
        },
        "category": {
            "type": "string",
            "default": "",
            "title": "The category Schema",
            "examples": [
                "Electronics"
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Smartphone"
            ]
        },
        "price": {
            "type": "number",
            "default": 0.0,
            "title": "The price Schema",
            "examples": [
                299.99
            ]
        },
        "isSold": {
            "type": "boolean",
            "default": false,
            "title": "The isSold Schema",
            "examples": [
                false
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "A gently used smartphone in excellent condition, with all original accessories."
            ]
        }
    },
    "additionalProperties": false
}